<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pavneet.android.boost">

    <uses-permission android:name="android.permission.INTERNET" />

    <!-- aa -->
    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <!--
                Register Activity, declare this activity as launcher
                mean first activity to be opened, on click of app icon
                -->

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.FoatingAndSnakBarActivity"
            android:label="@string/title_activity_foating_and_snak_bar" />
        <activity android:name=".activities.AnimateActivity" />
        <activity android:name=".activities.MaterialDesign"></activity>
    </application>

</manifest>